<view a:if="{{!showLoading}}" class="container-menu">

  <popup
    visible="{{basicVisible}}"
    position="{{position}}"
    animation="{{animation}}"
    onClose="handlePopupClose"
  >
    <view class="container-menu-popup">
      <view class="container-popup__user">
        <icon class="icon-close-menu" type="CloseOutline" onTap="handlePopupClose" />
        <text class="container-popup__name cn-h1">{{nombre}}</text>
        <text class="container-popup__document"> {{tipoDocumento}} : {{nit}}</text>
      </view>

      <view class="container-user-menu-access">
        <view class="container-menu-access__cards">

          <block a:for="{{menuAccess}}" a:for-index="index" a:for-item="item">
            <view
              class="menu-grid-access__cards-popup"
              onTap="onIconClick"
              data-index="{{index}}"
            >
              <view class="menu-access__card-icon">
                <view class="cards-menu__card-image">
                  <image
                    class="cards-container__card-menu-image"
                    src="{{item.iconAccess}}"
                  />
                </view>
              </view>
              <text class="cards-menu__card-text-access">{{item.titleAccess}}</text>
              <view class="cards-menu__icon__rigth">
                <icon class="cards-menu__icon" type="RightOutline" />
              </view>

            </view>
          </block>
        </view>
      </view>
      <view class="sign-out-container" onTap="handleOpenModal">
        <text>Cerrar sesión</text>
      </view>


    </view>
    <view class="terms-container">
      <view class="politics-container">
        <view onTap="goToTerms">
          <text >Términos y condiciones</text>
        </view>
        <!--<text style="padding-top:30rpx">Política de privacidad</text> -->
      </view>
      <divider lineHeight="2" lineColor="#B3B3B3" />
      <view class="menu-app-version">
        <text class="menu-app-version__text">Versión: 1.0.62</text>
      </view>

    </view>
    <modal-accept-cancel
      content="¿Quieres cerrar tu sesión?"
      visible="{{modalVisible}}"
      onClose="handleClose"
      primaryButtonText="Aceptar"
      secondaryButtonText="Cancelar"
      onPrimaryButtonTap="onSignOut"
      onSecondaryButtonTap="onCancelButtonTap"
    />
  </popup>


  <view class="menu-header">

    <view class="cn-header-enterprise-container">
      <view class="cn-header-back-gesture">
      </view>
      <image class="menu-header__image" src="/main/ui/assets/logo-claro-blanco.svg" />
    </view>
    <view class="menu-header__info">

      <view class="menu-header__info__horizontal">
        <text class="menu-header_info__text_name">{{nombre}}</text>
        <image
          data-position="right"
          onTap="handleShowMenu"
          class="menu-header_info__menu"
          src="/main/ui/assets/icons/menu.svg"
        />
      </view>

      <divider lineHeight="2" lineColor="#ffffff" />
      <text class="menu-header_info__text_nit"> {{tipoDocumento}} : {{nit}}</text>
    </view>



  </view>
  <view class="menu-grid-access">
    <view class="menu-grid-access__text cn-h1">
      <text>Tus servicios</text>
    </view>
    <view class="menu-grid-access__container">
      <block a:for="{{access}}" a:for-index="index" a:for-item="item">
        <view
          class="menu-grid-access__cards"
          onTap="navigateToMiniProgram"
          data-app-id="{{item.appId}}"
          data-pageUrl="{{item.pageUrl}}"
          data-index="{{index}}"
        >
          <view class="menu-grid-access__card-icon">
            <view class="cards-container__card-image">
              <image class="cards-container__card-icon-image" src="{{item.icon}}" />
            </view>
          </view>
          <text class="cards-container__card-text-access">{{item.text}}</text>
          <view a:if="{{item.hasBadge}}" class="badge-card">Nuevo</view>
        </view>
      </block>
    </view>
  </view>

  <!-- <view class="menu-banner">
    <image mode="scaleToFill" class="image-banner" src="data:image/jpeg;base64," />
  </view> -->

  <view class="page-section">
    <view class="page-section-demo">
      <swiper
        style="height:150px"
        class="image-scroll-view"
        previousMargin="10rpx"
        nextMargin="15rpx"
        onChange="swiper"
        indicator-dots="{{indicatorDots}}"
        current="{{toView}}"
      >
        <swiper-item a:for="{{order}}" key="swiper-item-{{index}}">
          <view
            id={{index}}
            class="container-info-datos-min"
            onTap="buttonTapNavigate"
            data-url="{{item.url}}"
          >
            <image mode="scaleToFill" src="{{item.image}}" />
          </view>
        </swiper-item>
      </swiper>
    </view>
  </view>

  <view class="container-btn-transition">
    <block a:for="{{orderIsTap}}" a:for-index="index" a:for-item="item">
      <button
        class="{{item ? 'stretch-button-active' : 'stretch-button'}}"
        onTap="buttonTap"
        data-item="{{index}}"
      >
      </button>
    </block>
  </view>

  <!--<view class="menu-banner" style="margin-top:40px"><image mode="scaleToFill" class="image-banner" src="data:image/jpeg;base64," /></view> -->

  <view class="menu_footer">
    <view class="menu_footer__container">
      <block a:for="{{footerItems}}" a:for-index="index" a:for-item="item">
        <view
          class="menu_footer__items {{item.isActive ? 'active' : ''}}"
          onTap="onFooterItemClick"
          data-index="{{index}}"
          data-path="{{item.path}}"
          data-app-id="{{item.appId}}"
        >
          <view class="menu_footer__items-container">
            <view class="menu_footer__items__cards">
              <image class="menu_footer__items__cards-icon" src="{{item.isActive ? item.iconActive : item.icon}}" />
              <text class="menu_footer__text">{{item.title}}</text>
            </view>
          </view>
        </view>
      </block>
    </view>
  </view>
</view>
<loading-component a:if="{{showLoading}}" />