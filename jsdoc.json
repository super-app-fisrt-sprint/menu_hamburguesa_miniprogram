{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["main"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "recurse": true,
      "destination": "./docs/"
    }
  }
