{
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "doc": "jsdoc -c jsdoc.json",
    "docs": " jsdoc --configure jsdoc.json --verbose",
    "lint-report": "npm run lint -- --format html --output-file ./eslint-report.html",
    "test": "jest --coverage --reporters=default --reporters=jest-junit",
    "test-watch": "jest --coverage --watch",
    "test:stage": "jest --passWithNoTests --runInBand --no-cache --findRelatedTests",
    "fix:styles": "stylelint main/**/*.acss --fix",
    "lint:styles": "stylelint main/**/*.acss",
    "prepare": "husky"
  },
  "dependencies": {
    "antd-mini": "^1.8.3",
    "claro-superapp-components": "^1.0.83",
    "jest": "^29.5.0"
  },
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "junit.xml",
    "usePathForSuiteName": "true"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "functions": 100,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "**/main/domain/**/*.{js,jsx}"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.2",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0"
  },
  "name": "claro_emrpresas_home_contenedor",
